<?php

/**
 * @file
 * Contains shc_migrate_survey_results.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function shc_migrate_survey_results_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the shc_migrate_survey_results module.
    case 'help.page.shc_migrate_survey_results':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Migrate survey data from Qualtrics API, .csv') . '</p>';
      return $output;

    default:
  }
}
